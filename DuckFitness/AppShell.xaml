<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DuckFitness.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DuckFitness"
    Shell.FlyoutBehavior="Disabled"
    Title="DuckFitness">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
        Title="Running"
        ContentTemplate="{DataTemplate local:Running}"
        Route="Running" />
</Shell>